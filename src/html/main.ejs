                                <img class="anim-down-from-overflow js-scroll-observe-el-inner" data-duration="0.5" data-delay="1" loading="lazy" src="images/content/background-img-03.jpg" alt="">
                            </div>
                        </div>
                        <div class="create-screen__labels anim-opacity js-scroll-observe-el" data-duration="0.5" data-delay="1.5">
                            <div class="create-screen__label">
                                Figma
                            </div>
                            <div class="create-screen__label">
                                Adobe Photoshop
                            </div>
                            <div class="create-screen__label">
                                Adobe Illustrator
                            </div>
                            <div class="create-screen__label">
                                Adobe After Effects
                            </div>
                            <div class="create-screen__label">
                                Sketch
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </section>
